{
  "files.exclude": {
    "out": true
  },
  "search.exclude": {
    "out": true
  },
  "typescript.tsc.autoDetect": "off",
  // Language: json
  "[json][jsonc][jsonl]": {
    "editor.defaultFormatter": "dprint.dprint",
    "editor.formatOnSave": true
  },
  // Language: markdown
  "[markdown]": {
    "editor.defaultFormatter": "dprint.dprint",
    "editor.formatOnSave": true
  },
  // Language: typescript
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  // Language: yaml
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  }
}
